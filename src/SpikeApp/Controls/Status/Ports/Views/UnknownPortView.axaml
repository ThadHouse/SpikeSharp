<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SpikeApp.Controls.Status.Ports.Views.UnknownPortView">
  <Grid ColumnDefinitions="*,*" >
    <TextBlock Grid.Column="0" HorizontalAlignment="Center" Text="{Binding PortName}"></TextBlock>
    <TextBlock Grid.Column="1" HorizontalAlignment="Center">Unknown</TextBlock>    
  </Grid>
</UserControl>
